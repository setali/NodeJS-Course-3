<div class="pagination">
  <ul>
    <% if (page > 1) { %>
    <li>
      <a href="?page=<%= page - 1 %>"> Perv </a>
    </li>
    <% } %> <% for(let i = 1; i <= pages; i++) { %>
    <li>
      <a href="?page=<%= i %>"> <%= i %> </a>
    </li>
    <% } %> <% if (page < pages) { %>
    <li>
      <a href="?page=<%= page + 1 %>"> Next </a>
    </li>
    <% } %>
  </ul>
</div>
